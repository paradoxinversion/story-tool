{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
           "jsx": true
        }
    },
    "env":{
            "browser": true,
            "node": true
        },
    "plugins":[
        "react"
    ],
    "rules": {
        "semi": 2
    }
    
}